<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiuye.sona.product.dao.ProductDao">
    <!--查询商品信息-->
    <select id="queryProduct" resultType="ProductEntity" parameterType="String">
        SELECT
            product_id,
            product_name,
            sku
        FROM
            sona_product
    </select>
</mapper>
